#include <iostream>
using namespace std;

int main()
{
    int n, x, index, flag=0;
    cout<<"Enter number of elements: ";
    cin>>n;
    int arr[n];
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    cout<<"Enter element to be searched: ";
    cin>>x;
    for(int i=0; i<n; i++){
        if(arr[i]==x){
            flag=1;
            index = i;
        }
    }
    if(flag==1){
        cout<<x<<" found at index "<<index<<endl;
    }
    else{
        cout<<x<<" not found"<<endl;
    }
    

    return 0;
}
